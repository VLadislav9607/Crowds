{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "target": "ES2020",
    "useDefineForClassFields": true,
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "module": "ESNext",
    "skipLibCheck": true,
    "moduleResolution": "bundler",
    "allowImportingTsExtensions": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "baseUrl": ".",
    "paths": {
      "@assets": ["./src/assets/index.ts"],
      "@components": ["./src/components/index.ts"],
      "@constants": ["./src/constants/index.ts"],
      "@helpers": ["./src/helpers/index.ts"],
      "@hooks": ["./src/hooks/index.ts"],
      "@navigation": ["./src/navigation/index.ts"],
      "@services": ["./src/services/index.ts"],
      "@styles": ["./src/styles/index.ts"],
      "@types": ["./src/types/index.ts"],
      "@ui": ["./src/ui/index.ts"],
      "@store": ["./src/store/index.ts"],
      "@modules/common": ["./src/modules/common/index.ts"],
      "@modules/profile": ["./src/modules/profile/index.ts"],
      "@modules/onboarding": ["./src/modules/onboarding/index.ts"],
      "@modules/events": ["./src/modules/events/index.ts"],
      "@modules/kyc": ["./src/modules/kyc/index.ts"],
      "@actions": ["./src/actions/index.ts"],
      "@utils": ["./src/utils/index.ts"]
    }
  }
}
